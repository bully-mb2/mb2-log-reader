                <xs:element name="color2" type="xs:int"/>
                <xs:element name="siegeclass" type="xs:string"/>
                <xs:element name="saber1" type="xs:string"/>
                <xs:element name="saber2" type="xs:string"/>
                <xs:element name="desired_team" type="Team"/>
                <xs:element name="model_variant" type="xs:int"/>
