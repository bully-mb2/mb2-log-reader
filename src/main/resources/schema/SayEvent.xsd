    <xs:include schemaLocation="Shared.xsd"/>

    <xs:complexType name="SayEvent">
        <xs:all>
            <xs:element name="slot" type="PlayerSlot"/>
            <xs:element name="message" type="xs:string"/>
            <xs:element name="chat" type="Chat"/>
